<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>Serializer</title>
    <parser>
        <target>doc</target>
        <encoding>utf8</encoding>
        <extensions>
            <extension>php</extension>
        </extensions>
    </parser>
    <transformer>
        <target>doc</target>
    </transformer>
    <transformations>
        <template name="responsive-twig"/>
    </transformations>
    <files>
        <directory>src</directory>
        <ignore>src/Tests/*</ignore>
    </files>
</phpdoc>